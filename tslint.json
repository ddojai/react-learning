{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name": [
      false
    ],
    "member-access": [
      false
    ],
    "no-empty-interface": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "jsx-no-lambda": false,
    "interface-over-type-literal": false,
    "max-classes-per-file": false,
    "no-angle-bracket-type-assertion": false
  }
}
